{
  "name": "clock",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "typesync",
    "start": "concurrently \"npm run develop\" \"npm run server\"",
    "server": "lite-server -c server.json",
    "develop": "npm run webpack -- --devtool inline-source-map --watch",
    "webpack": "NODE_ENV=development webpack --config webpack.config.js --colors --display-error-details --progress --mode development",
    "minify": "NODE_ENV=production webpack --config webpack.config.js --colors --display-error-details --progress --mode production"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@testing-library/react": "^11.0.2",
    "@types/babel__core": "^7.1.11",
    "@types/babel__preset-env": "^7.9.1",
    "@types/concurrently": "^5.2.1",
    "@types/core-js": "^2.5.4",
    "@types/eslint": "^7.2.4",
    "@types/node": "^14.6.4",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^16.9.49",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/remote-redux-devtools": "^0.5.4",
    "@types/terser-webpack-plugin": "^5.0.1",
    "@types/webfontloader": "^1.6.32",
    "@types/webpack": "^4.41.22",
    "axios": "^0.20.0",
    "babel-loader": "^8.1.0",
    "beautiful-react-hooks": "^0.30.0",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "css-loader": "^4.2.2",
    "eslint": "^7.11.0",
    "eslint-plugin-react-app": "^6.2.2",
    "lite-server": "^2.5.4",
    "node-sass": "^4.14.1",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.16",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",

{
  "name": "clock",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "typesync",
    "start": "concurrently \"npm run develop\" \"npm run server\"",
    "server": "lite-server -c server.json",
    "develop": "npm run webpack -- --devtool inline-source-map --watch",
    "webpack": "BABEL_ENV=development NODE_ENV=development webpack --config webpack.config.js --color --mode development --progress",
    "minify": "BABEL_ENV=production NODE_ENV=production webpack --config webpack.config.js --color --progress --mode production"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.5",
    "@types/concurrently": "^6.0.1",
    "@types/eslint": "^7.2.7",
    "@types/node": "^14.14.36",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^17.0.3",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^17.0.3",
    "@types/terser-webpack-plugin": "^5.0.3",
    "@types/uglifyjs-webpack-plugin": "^1.1.1",
    "@types/webfontloader": "^1.6.32",
    "@types/webpack": "^5.0.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "axios": "^0.21.1",
    "beautiful-react-hooks": "^0.31.1",
    "concurrently": "^6.0.0",
    "css-loader": "^5.2.0",
    "eslint": "^7.22.0",
    "eslint-plugin-react-app": "^6.2.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lite-server": "^2.6.1",
    "node-sass": "^5.0.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
    "typesync": "^0.8.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webfontloader": "^1.6.28",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "recoil": "^0.1.3",
    "terser-webpack-plugin": "^5.1.1"
  }
}
